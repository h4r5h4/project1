function checkEmail() {
if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(myForm.email.value)){
return true;
}
return false; 
}
function ValidateEmail(){
var emailID=document.myForm.email;

var rollno=document.myForm.rollno;

var name=document.myForm.name;

var uname=document.myForm.uname;
var role=document.myForm.role;
var pwd=document.myForm.pwd;
var cpwd=document.myForm.cpwd;

if ((rollno.value==null)||(rollno.value=="")){
alert("Please Enter Roll No")
rollno.focus()
return false
}
if ((uname.value==null)||(uname.value=="")){
alert("Please Enter Username")
uname.focus()
return false
}
if ((pwd.value==null)||(pwd.value=="")){
alert("Please Enter Password")
pwd.focus()
return false
}
if ((cpwd.value==null)||(cpwd.value=="")){
alert("Please Enter Confirmation Password")
cpwd.focus()
return false
}
if ((name.value==null)||(name.value=="")){
alert("Please Enter Name")
name.focus()
return false
}
if ((emailID.value==null)||(emailID.value=="")){
alert("Please Enter Email ID")
emailID.focus()
return false
}
if ((role.value==null)||(role.value=="")){
alert("Please Enter Role")
role.focus()
return false
}

if (pwd.value!=cpwd.value){
alert("Passwords Do not match")
pwd.focus()
return false
}
if (checkEmail(emailID.value)==false){
emailID.value=""
alert("Invalid Email Address");
emailID.focus()
return false
}
return true
}
</script>
